generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  createdAt      DateTime? @db.Date
  email          String
  hashedPassword String?
  name           String
  profile        String?
  updatedAt      DateTime? @db.Date
}

model b {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
